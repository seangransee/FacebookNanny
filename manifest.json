{
  "name": "Facebook Nanny",
  "version": "0.4",
  "description": "Disables Facebook unless you have notifications or need to chat with someone.",
  "permissions": [ "cookies", "*://*.facebook.com/*" ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "main.js" ],
    "css": ["styles.css"],
    "matches": [ "*://www.facebook.com/*"]
  } ]
}